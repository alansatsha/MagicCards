<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Theme.MagicCards" parent="Theme.MaterialComponents.DayNight.DarkActionBar">
        <!-- Primary brand color. -->
        <item name="colorPrimary">@color/purple_500</item>
        <item name="colorPrimaryVariant">@color/purple_700</item>
        <item name="colorOnPrimary">@color/white</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">@color/teal_200</item>
        <item name="colorSecondaryVariant">@color/teal_700</item>
        <item name="colorOnSecondary">@color/black</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor" tools:targetApi="l">@color/mcol</item>
        <!-- Customize your theme here. -->
    </style>

    <style name="Theme.MagicCards.NoActionBar">
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
    </style>



    <style name="ShapeAppearanceOverlay.App.CornerSize10Percent" parent="">
        <item name="cornerSize">50dp</item>
        <item name="cornerSizeBottomRight">10dp</item>
        <item name="cornerSizeBottomLeft">10dp</item>
    </style>

    <style name="ShapeAppearanceOverlay.App.CornerSize10Percente" parent="">
        <item name="cornerSize">20dp</item>
        <item name="cornerSizeBottomRight">5dp</item>
        <item name="cornerSizeBottomLeft">5dp</item>
    </style>

    <style name="ShapeAppearanceOverlay.App.CornerSize10Percentec" parent="">
        <item name="cornerSize">35dp</item>
    </style>

    <style name="Button.lStyle" parent="ThemeOverlay.AppCompat">
        <item name="colorAccent">@color/muuuu</item>
        <item name="colorPrimary">@color/muuuu</item>
    </style>

    <style name="Theme.MagicCards.AppBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar" />

    <style name="Theme.MagicCards.PopupOverlay" parent="ThemeOverlay.AppCompat.Light" />


</resources>